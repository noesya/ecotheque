<%= content_for :title, "#{User::Log.model_name.human(count: 2)} (#{@logs.total_count})" %>

<%= render 'list', logs: @logs %>
<%= paginate @logs, theme: 'bootstrap-5' %>

<% content_for :buttons do %>
  <%= link_to t('admin.export'), request.params.merge(format: "xlsx"), class: button_classes %>
<% end %>